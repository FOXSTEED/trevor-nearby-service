				"exec": [
					"function getRandomIntInclusive(min, max) {",
					"  min = Math.ceil(min);",
					"  max = Math.floor(max);",
					"  return Math.floor(Math.random() * (max - min + 1)) + min;",
					"}",
					"",
					"pm.variables.set(\"id\", getRandomIntInclusive(1, 2999999))"
				]
			}
